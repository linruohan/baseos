
[chef_stable]
name=chef_stable
mirrorlist=http://io.dev/?release=$releasever&arch=$basearch&repo=chef_stable
repo_gpgcheck=0
gpgcheck=0
enabled=1
gpgkey=https://mirrors.mudbox.dev/gpg.key
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt

[chef_stable-source]
name=chef_stable-source
mirrorlist=https://packagecloud.io/chef/stable/el/6/SRPMS
repo_gpgcheck=1
gpgcheck=0
enabled=0
gpgkey=https://mirrors.mudbox.dev/gpg.key
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt

